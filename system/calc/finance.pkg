// Modified Internal Rate of Return (MIRR)
void MIRR(initialInvestment, cashFlows, reinvestmentRate, financingRate) {
    let positiveCashFlows = 0
    let negativeCashFlows = 0

    for cashFlow in cashFlows {
        if cashFlow > 0 {
            positiveCashFlows += cashFlow * pow(1 + reinvestmentRate, cashFlows.indexOf(cashFlow))
        } else {
            negativeCashFlows += cashFlow * pow(1 + financingRate, cashFlows.indexOf(cashFlow))
        }
    }

    let pvPositive = initialInvestment + positiveCashFlows
    let pvNegative = negativeCashFlows

    return pow(pvPositive / abs(pvNegative), 1 / cashFlows.count) - 1
}

