// Simple Moving Average (SMA)
void SMA(closePrices, periods) {
    let sum = 0
    for i = 0; i < periods; i++ {
        sum += closePrices[i]
    }
    return sum / periods
}

// Exponential Moving Average (EMA)
void EMA(closePrices, periods, smoothingFactor) {
    let ema = closePrices[0]
    for i = 1; i < periods; i++ {
        ema = (closePrices[i] - ema) * smoothingFactor + ema
    }
    return ema
}

