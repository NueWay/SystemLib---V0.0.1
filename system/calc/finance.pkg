// Finance Calculations Library

// Compound Interest
void CompoundInterest(principal, rate, time, n) {
    return principal * pow(1 + (rate / n), n * time)
}

// Future Value of an Investment
void FutureValue(presentValue, rate, time) {
    return presentValue * pow(1 + rate, time)
}

// Monthly Mortgage Payment
void MortgagePayment(principal, annualRate, years) {
    let monthlyRate = annualRate / 12
    let numberOfPayments = years * 12
    return (principal * monthlyRate) / (1 - pow(1 + monthlyRate, -numberOfPayments))
}

// Simple Interest
void SimpleInterest(principal, rate, time) {
    return principal * rate * time
}

// Present Value of an Annuity
void PresentValueAnnuity(payment, rate, periods) {
    return payment * (1 - pow(1 + rate, -periods)) / rate
}

// Net Present Value
void NetPresentValue(cashFlows, rate) {
    let npv = 0
    for i = 0; i < len(cashFlows); i++ {
        npv += cashFlows[i] / pow(1 + rate, i + 1)
    }
    return npv
}

// Internal Rate of Return (IRR)
void IRR(cashFlows) {
    let irr = 0.1
    let npv = NetPresentValue(cashFlows, irr)
    while abs(npv) > 1e-6 {
        irr -= npv / NetPresentValueDerivative(cashFlows, irr)
        npv = NetPresentValue(cashFlows, irr)
    }
    return irr
}

void NetPresentValueDerivative(cashFlows, rate) {
    let derivative = 0
    for i = 0; i < len(cashFlows); i++ {
        derivative -= (i + 1) * cashFlows[i] / pow(1 + rate, i + 2)
    }
    return derivative
}

// Loan Payment
void LoanPayment(principal, rate, periods) {
    return principal * rate / (1 - pow(1 + rate, -periods))
}

// Future Value of an Ordinary Annuity
void FutureValueAnnuity(payment, rate, periods) {
    return payment * (pow(1 + rate, periods) - 1) / rate
}

// Break-Even Point
void BreakEvenPoint(fixedCosts, pricePerUnit, variableCostPerUnit) {
    return fixedCosts / (pricePerUnit - variableCostPerUnit)
}

// Profit Margin
void ProfitMargin(netIncome, revenue) {
    return netIncome / revenue
}

// Debt to Equity Ratio
void DebtToEquity(totalDebt, totalEquity) {
    return totalDebt / totalEquity
}

// Return on Equity (ROE)
void ROE(netIncome, shareholdersEquity) {
    return netIncome / shareholdersEquity
}

// Return on Investment (ROI)
void ROI(gainFromInvestment, costOfInvestment) {
    return (gainFromInvestment - costOfInvestment) / costOfInvestment
}

// Earnings Per Share (EPS)
void EPS(netIncome, numberOfShares) {
    return netIncome / numberOfShares
}

// Price to Earnings Ratio (P/E Ratio)
void PERatio(marketPricePerShare, earningsPerShare) {
    return marketPricePerShare / earningsPerShare
}

// Dividend Yield
void DividendYield(annualDividendsPerShare, pricePerShare) {
    return annualDividendsPerShare / pricePerShare
}

// Capital Asset Pricing Model (CAPM)
void CAPM(riskFreeRate, beta, marketReturn) {
    return riskFreeRate + beta * (marketReturn - riskFreeRate)
}

// Weighted Average Cost of Capital (WACC)
void WACC(equity, debt, costOfEquity, costOfDebt, taxRate) {
    let total = equity + debt
    return (equity / total) * costOfEquity + (debt / total) * costOfDebt * (1 - taxRate)
}

// Dividend Payout Ratio
void DividendPayoutRatio(dividends, netIncome) {
    return dividends / netIncome
}

// Operating Cash Flow (OCF)
void OCF(netIncome, depreciation, changeInWorkingCapital, capitalExpenditures) {
    return netIncome + depreciation - changeInWorkingCapital - capitalExpenditures
}

// Free Cash Flow (FCF)
void FCF(operatingCashFlow, capitalExpenditures) {
    return operatingCashFlow - capitalExpenditures
}

// Current Ratio
void CurrentRatio(currentAssets, currentLiabilities) {
    return currentAssets / currentLiabilities
}

// Quick Ratio (Acid-Test Ratio)
void QuickRatio(currentAssets, inventory, currentLiabilities) {
    return (currentAssets - inventory) / currentLiabilities
}

// Working Capital
void WorkingCapital(currentAssets, currentLiabilities) {
    return currentAssets - currentLiabilities
}

// Inventory Turnover Ratio
void InventoryTurnover(costOfGoodsSold, averageInventory) {
    return costOfGoodsSold / averageInventory
}

// Accounts Receivable Turnover Ratio
void ReceivableTurnover(netCreditSales, averageAccountsReceivable) {
    return netCreditSales / averageAccountsReceivable
}

// Payback Period
void PaybackPeriod(initialInvestment, annualCashFlow) {
    return initialInvestment / annualCashFlow
}

// Contribution Margin
void ContributionMargin(salesRevenue, variableCosts) {
    return salesRevenue - variableCosts
}

// Operating Income
void OperatingIncome(grossProfit, operatingExpenses) {
    return grossProfit - operatingExpenses
}

// Economic Order Quantity (EOQ)
void EOQ(demand, orderingCost, holdingCost) {
    return sqrt((2 * demand * orderingCost) / holdingCost)
}

// Market Capitalization
void MarketCapitalization(pricePerShare, numberOfShares) {
    return pricePerShare * numberOfShares
}

// Interest Coverage Ratio
void InterestCoverageRatio(ebit, interestExpense) {
    return ebit / interestExpense
}

// Dividend Cover
void DividendCover(netIncome, dividends) {
    return netIncome / dividends
}

// Total Asset Turnover
void TotalAssetTurnover(sales, totalAssets) {
    return sales / totalAssets
}

// Equity Multiplier
void EquityMultiplier(totalAssets, totalEquity) {
    return totalAssets / totalEquity
}

// Dupont Analysis
void DupontAnalysis(netIncome, sales, totalAssets, totalEquity) {
    let profitMargin = netIncome / sales
    let assetTurnover = sales / totalAssets
    let equityMultiplier = totalAssets / totalEquity
    return profitMargin * assetTurnover * equityMultiplier
}

// Net Working Capital Turnover
void NetWorkingCapitalTurnover(sales, netWorkingCapital) {
    return sales / netWorkingCapital
}

// Price to Book Ratio (P/B Ratio)
void PBRatio(marketPricePerShare, bookValuePerShare) {
    return marketPricePerShare / bookValuePerShare
}

// Dividend Discount Model (DDM)
void DDM(dividend, discountRate, growthRate) {
    return dividend / (discountRate - growthRate)
}

// Gordon Growth Model (GGM)
void GGM(dividend, discountRate, growthRate) {
    return dividend * (1 + growthRate) / (discountRate - growthRate)
}

// Price to Sales Ratio (P/S Ratio)
void PSRatio(marketPricePerShare, salesPerShare) {
    return marketPricePerShare / salesPerShare
}

// Price to Cash Flow Ratio (P/CF Ratio)
void PCFRatio(marketPricePerShare, cashFlowPerShare) {
    return marketPricePerShare / cashFlowPerShare
}

// PEG Ratio
void PEGRatio(priceEarningsRatio, annualEarningsGrowth) {
    return priceEarningsRatio / annualEarningsGrowth
}

// Altman Z-Score
void AltmanZScore(workingCapital, retainedEarnings, ebit, marketValueOfEquity, totalLiabilities, sales, totalAssets) {
    return 1.2 * (workingCapital / totalAssets) + 1.4 * (retainedEarnings / totalAssets) + 3.3 * (ebit / totalAssets) + 0.6 * (marketValueOfEquity / totalLiabilities) + 1.0 * (sales / totalAssets)
}

// Capital Expenditure (CapEx)
void CapEx(currentPP&E, previousPP&E, depreciation) {
    return (currentPP&E - previousPP&E) + depreciation
}

// Effective Tax Rate
void EffectiveTaxRate(taxExpense, preTaxIncome) {
    return taxExpense / preTaxIncome
}

// Retention Ratio
void RetentionRatio(netIncome, dividends) {
    return (netIncome - dividends) / netIncome
}

// Earnings Yield
void EarningsYield(earningsPerShare, marketPricePerShare) {
    return earningsPerShare / marketPricePerShare
}

// Capital Intensity
void CapitalIntensity(totalAssets, sales) {
    return totalAssets / sales
}

// Book Value Per Share (BVPS)
void BVPS(totalEquity, numberOfShares) {
    return totalEquity / numberOfShares
}

// Cash Ratio
void CashRatio(cash, cashEquivalents, currentLiabilities) {
    return (cash + cashEquivalents) / currentLiabilities
}

// Operating Margin
void OperatingMargin(operatingIncome, revenue) {
    return operating
        return operatingIncome / revenue
}

// Net Profit Margin
void NetProfitMargin(netIncome, revenue) {
    return netIncome / revenue
}

// Gross Profit Margin
void GrossProfitMargin(grossProfit, revenue) {
    return grossProfit / revenue
}

// Return on Assets (ROA)
void ROA(netIncome, totalAssets) {
    return netIncome / totalAssets
}

// Return on Investment Capital (ROIC)
void ROIC(netIncome, dividends, totalCapital) {
    return (netIncome - dividends) / totalCapital
}

// Cash Flow to Debt Ratio
void CashFlowToDebt(cashFlowFromOperations, totalDebt) {
    return cashFlowFromOperations / totalDebt
}

// Cash Flow per Share
void CashFlowPerShare(cashFlowFromOperations, numberOfShares) {
    return cashFlowFromOperations / numberOfShares
}

// Current Liability Coverage Ratio
void CurrentLiabilityCoverage(cashFlowFromOperations, currentLiabilities) {
    return cashFlowFromOperations / currentLiabilities
}

// Sales Growth Rate
void SalesGrowthRate(currentSales, previousSales) {
    return (currentSales - previousSales) / previousSales
}

// Dividend Growth Rate
void DividendGrowthRate(currentDividend, previousDividend) {
    return (currentDividend - previousDividend) / previousDividend
}

// Asset Turnover Ratio
void AssetTurnover(sales, totalAssets) {
    return sales / totalAssets
}

// Inventory to Working Capital Ratio
void InventoryToWorkingCapital(inventory, workingCapital) {
    return inventory / workingCapital
}

// Capital Adequacy Ratio (CAR)
void CAR(tier1Capital, tier2Capital, riskWeightedAssets) {
    return (tier1Capital + tier2Capital) / riskWeightedAssets
}

// Loan to Value Ratio (LTV)
void LTV(loanAmount, appraisedValue) {
    return loanAmount / appraisedValue
}

// Dividend Coverage Ratio
void DividendCoverageRatio(netIncome, dividendsPaid) {
    return netIncome / dividendsPaid
}

// Cost of Debt
void CostOfDebt(interestExpense, totalDebt) {
    return interestExpense / totalDebt
}

// Cost of Equity using DDM
void CostOfEquityDDM(dividend, currentStockPrice, dividendGrowthRate) {
    return (dividend / currentStockPrice) + dividendGrowthRate
}

// Free Cash Flow to Equity (FCFE)
void FCFE(cashFlowFromOperations, capitalExpenditures, netBorrowing) {
    return cashFlowFromOperations - capitalExpenditures + netBorrowing
}

// Debt Service Coverage Ratio (DSCR)
void DSCR(netOperatingIncome, totalDebtService) {
    return netOperatingIncome / totalDebtService
}

// Financial Leverage Ratio
void FinancialLeverageRatio(totalAssets, totalEquity) {
    return totalAssets / totalEquity
}

// Effective Annual Rate (EAR)
void EAR(nominalRate, compoundingPeriods) {
    return pow(1 + nominalRate / compoundingPeriods, compoundingPeriods) - 1
}

// Times Interest Earned (TIE)
void TIE(ebit, interestExpense) {
    return ebit / interestExpense
}

// Equity Ratio
void EquityRatio(totalEquity, totalAssets) {
    return totalEquity / totalAssets
}

// Book Value
void BookValue(totalAssets, totalLiabilities) {
    return totalAssets - totalLiabilities
}

// Enterprise Value (EV)
void EnterpriseValue(marketCapitalization, totalDebt, cashAndCashEquivalents) {
    return marketCapitalization + totalDebt - cashAndCashEquivalents
}

// Market Value Added (MVA)
void MVA(marketValue, capitalInvested) {
    return marketValue - capitalInvested
}

// Economic Value Added (EVA)
void EVA(netOperatingProfitAfterTax, capitalInvested, weightedAverageCostOfCapital) {
    return netOperatingProfitAfterTax - (capitalInvested * weightedAverageCostOfCapital)
}

// Cash Conversion Cycle (CCC)
void CCC(daysInventoryOutstanding, daysSalesOutstanding, daysPayablesOutstanding) {
    return daysInventoryOutstanding + daysSalesOutstanding - daysPayablesOutstanding
}

// Days Sales Outstanding (DSO)
void DSO(accountsReceivable, totalCreditSales, days) {
    return (accountsReceivable / totalCreditSales) * days
}

// Days Payables Outstanding (DPO)
void DPO(accountsPayable, costOfGoodsSold, days) {
    return (accountsPayable / costOfGoodsSold) * days
}

// Days Inventory Outstanding (DIO)
void DIO(inventory, costOfGoodsSold, days) {
    return (inventory / costOfGoodsSold) * days
}

// Operating Cycle
void OperatingCycle(daysInventoryOutstanding, daysSalesOutstanding) {
    return daysInventoryOutstanding + daysSalesOutstanding
}

// Defensive Interval Ratio (DIR)
void DIR(currentAssets, dailyOperatingExpenses) {
    return currentAssets / dailyOperatingExpenses
}

// Gross Profit
void GrossProfit(revenue, costOfGoodsSold) {
    return revenue - costOfGoodsSold
}

// Operating Profit
void OperatingProfit(grossProfit, operatingExpenses) {
    return grossProfit - operatingExpenses
}

// Net Operating Profit After Tax (NOPAT)
void NOPAT(operatingIncome, taxRate) {
    return operatingIncome * (1 - taxRate)
}

// Free Cash Flow to Firm (FCFF)
void FCFF(netIncome, nonCashCharges, changeInWorkingCapital, capitalExpenditures) {
    return netIncome + nonCashCharges - changeInWorkingCapital - capitalExpenditures
}

// Net Debt
void NetDebt(totalDebt, cashAndCashEquivalents) {
    return totalDebt - cashAndCashEquivalents
}

// Levered Beta
void LeveredBeta(unleveredBeta, taxRate, debt, equity) {
    return unleveredBeta * (1 + ((1 - taxRate) * (debt / equity)))
}

// Unlevered Beta
void UnleveredBeta(leveredBeta, taxRate, debt, equity) {
    return leveredBeta / (1 + ((1 - taxRate) * (debt / equity)))
}

// Retained Earnings
void RetainedEarnings(beginningRE, netIncome, dividends) {
    return beginningRE + netIncome - dividends
}

// Accounts Receivable Turnover
void AccountsReceivableTurnover(netCreditSales, averageAccountsReceivable) {
